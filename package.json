    "body-parser": "^1.18.2",
    "bookshelf": "^0.10.4",
    "bookshelf-upsert": "^0.1.4",
    "bulma": "^0.6.1",
    "discord.js": "^11.2.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "knex": "^0.13.0",
    "moment": "^2.19.2",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-steam": "^1.0.8",
    "paypal-rest-sdk": "^1.7.1",
    "pug": "^2.0.0-rc.4",
    "request": "^2.83.0",
